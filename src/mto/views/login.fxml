                        prefHeight="31.0" prefWidth="215.0" promptText="Password" styleClass="field"
                        unFocusColor="WHITE"/>
      <JFXButton id="login" layoutX="39.0" layoutY="288.0" onAction="#showHome" prefHeight="50.0" prefWidth="215.0"
                 text=" Login "/>
      <Label layoutX="93.0" layoutY="57.0" text="Log in First" textFill="#fcfcfc">
         <font>
            <Font size="20.0"/>
         </font>
      </Label>
   </Pane>
